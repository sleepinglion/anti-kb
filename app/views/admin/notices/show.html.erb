<div class="sl_show">
	<p>
		<b><%=t(:title,scope:[:activerecord,:attributes,:notice]) %></b>
		<%= @notice.title %>
	</p>
	<p>
		<b><%=t(:enable,scope:[:activerecord,:attributes,:notice]) %></b>
		<%=raw flagToString(@notice.enable) %>
	</p>
	<p>
  	<b><%=t(:label_created_at) %></b>
  	<%= @notice.created_at %>
  </p>
  <p>
  	<b><%=t(:label_updated_at) %></b>
  	<%= @notice.created_at %>
  </p>
</div>
<% if can? :update, Report %>
<%= link_to t(:link_edit), edit_admin_notice_path(@notice) %> |
<% end %>
<%= link_to t(:link_list), admin_notices_path %>
